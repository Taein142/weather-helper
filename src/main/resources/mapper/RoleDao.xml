<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.icia.weatherhelper.dao.RoleDao">
    <select id="findUserByRoleId" parameterType="Integer" resultType="UserDTO">
        SELECT * FROM user
        WHERE role_id=#{userRole}
    </select>

</mapper>